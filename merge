#include <iostream>

void merge(int* inicio, int* fin) {
    for (int i = 0; i < 4; ++i) {
        for (int j = 0; j < 4 - i; ++j) {
            if (inicio[j] > inicio[j + 1]) {
                int temp = inicio[j];
                inicio[j] = inicio[j + 1];
                inicio[j + 1] = temp;
            }
        }
    }

    for (int i = 0; i < 4; ++i) {
        for (int j = 5; j < 9 - i; ++j) {
            if (inicio[j] > inicio[j + 1]) {
                int temp = inicio[j];
                inicio[j] = inicio[j + 1];
                inicio[j + 1] = temp;
            }
        }
    }
}

int main() {
    int arr[10] = { 9, 1, 8, 2, 7, 3, 6, 4, 5, 0 };

    std::cout << "Array original: ";
    for (int i = 0; i < 10; ++i) {
        std::cout << arr[i] << " ";
    }
    std::cout << std::endl;

   merge(arr, arr + 9);

    std::cout << "Array ordenado parcialmente: ";
    for (int i = 0; i < 10; ++i) {
        std::cout << arr[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
